<!-- change to prev imp: use torque control -->
<mujoco>

  <include file="scene.xml"/>
  <compiler angle="radian" coordinate="local" eulerseq="XYZ"/>
  <size njmax="5000" nconmax="250"/>

  <!-- set to default values at the moment -->
  <default class="contact">
    <default class="hole">
      <geom type="box" mass="0.0099" size="0.02500 0.00047 0.03000"/>
    </default>

    <default class="peg">
      <!-- size = (radius, half-heihgt) -->
      <geom type="box" mass="0.5" size="0.0149 0.0149 0.02" euler="0 0 0"
                priority="1" condim="6" friction="0.5 0.005 0.001"
                solimp="0.5 0.95 0.001 0.1 6" solref="0.02 1"/>
      <!-- <pair condim="6" friction="0.01 0.01 0.005 0.0001 0.0001"
        solimp="0.9 0.95 0.001 0.5 2" solref="0.02 1"
        margin="0" gap="0" /> -->
    </default>
  </default>

  <!-- triangular prism mesh -->
  <asset>
    <mesh name="prism" vertex="0.0150 0.00047 0.0200  0.0150 0.00047 0.0400  0.06500 0.00047 0.02000  0.06500 0.00047 0.04000  0.06490 0.00361 0.02000  0.06490 0.00361 0.04000"/>
  </asset>

  <!-- panda robot -->
  <include file="../franka_sim/assets/assets.xml"/>
  <compiler meshdir=""/>

  <worldbody>
      <include file="chain_peg.xml"/>
  </worldbody>

  <include file='../franka_sim/assets/force_actuator.xml'/>


  <!-- default class to adjust the paramters of peg -->
  <worldbody>
    <!-- <body>
      <geom name="test" type="box" pos="0.3 0 0.06" size = "0.05 0.05 0.005" />
    </body> -->
    <body name="hole" pos="0.53 0.012 0.1088">
      <geom name="base" type="box" mass="0.1" pos="0 0 0.01" size="0.1 0.1 0.01"/>
      <geom name="block1" type="box" mass="0.1" pos="0.0575 -0.0425 0.03" size="0.0425 0.0575 0.01"/>
      <geom name="block2" type="box" mass="0.1" pos="-0.0575 0.0425 0.03" size="0.0425 0.0575 0.01"/>
      <geom name="block3" type="box" mass="0.1" pos="0.0425 0.0575 0.03" size="0.0575 0.0425 0.01"/>
      <geom name="block4" type="box" mass="0.1" pos="-0.0425 -0.0575 0.03" size="0.0575 0.0425 0.01"/>
    </body>
  </worldbody>

</mujoco>
